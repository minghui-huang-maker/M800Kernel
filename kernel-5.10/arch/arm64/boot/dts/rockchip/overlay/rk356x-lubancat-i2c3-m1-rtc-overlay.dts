/dts-v1/;
/plugin/;

/ {
	fragment@0 {
		target = <&i2c3>;

		__overlay__ {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&i2c3m1_xfer>;
			#address-cells = <1>;
			#size-cells = <0>;

			pcf8563: pcf8563@51 {
				compatible = "nxp,pcf8563";
				reg = <0x51>;
			};

			ds1307: ds1307@68 {
				compatible = "dallas,ds1307";
				reg = <0x68>;
			};
		};
	};

	fragment@1 {
		target = <&rk809>;

		__overlay__ {
			rtc {
				status = "disabled";
			};
		};
	};
};
